@model Alboraq.MobileApp.WebApi.Models.MVC.Admin.UpdateRoleBindingModel

@using (Html.BeginForm("", "", FormMethod.Post, new { id = "frmUpdateRole" }))
{
    @Html.HiddenFor(role=>role.RoleID)
    <div class="form-group">
        @Html.LabelFor(role => role.RoleName)
        @Html.TextBoxFor(role => role.RoleName, new { @class = "form-control" })
    </div>
    <div class="form-group">
        <button type="submit" id="btnUpdateRole" class="btn btn-default">Update</button>
    </div>
}

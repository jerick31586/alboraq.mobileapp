@model IEnumerable<Alboraq.MobileApp.EF.ApplicationUser>

@if (Model.Count() > 0)
{
    <div class="body-content">
        <br /><br />
    <button type="button" id="btnAddUserShowDialog" 
            class="btn btn-default"
            data-target="#appModal"
            data-toggle="modal"
            data-backdrop="static"
            data-keyboard="false">Add new user</button><br /><br />

    <table class="table table-bordered table-condensed table-hover">
        <thead>
            <tr>
                <th>Username</th>
                <th>Name</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.UserName</td>
                    <td>@user.Name</td>
                    <td>@user.Email</td>
                </tr>
            }
        </tbody>
    </table>
        </div>
}
else
{

}

